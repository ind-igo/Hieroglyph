{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": [
        "svelte3"
    ],
    "overrides": [
        {
            "files": ["*.svelte"],
            "processor: "svelte3/svelte3"
        }
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
    }
}
